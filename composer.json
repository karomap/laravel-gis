{
    "name": "karomap/laravel-gis",
    "description": "GIS for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "gis"
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.0",
        "geo-io/geojson": "dev-main",
        "geo-io/wkb-parser": "dev-main",
        "geo-io/wkt-parser": "dev-main",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Karomap\\LaravelGIS\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Karomap\\LaravelGIS\\GISServiceProvider"
            ]
        }
    }
}
